    "df = pd.read_csv('C:\\\\Users\\\\pavan\\\\Downloads\\\\raw_data_v11_3.csv')\n",
    "df['Date of Birth'] = pd.to_datetime(df['Date of Birth'], errors='coerce')\n",
    "print(\"Number of missing values in 'Date of Birth':\", df['Date of Birth'].isnull().sum())\n",
    "df = df.dropna(subset=['Date of Birth'])\n",
